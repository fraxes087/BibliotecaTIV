@model List<Biblioteca.UserInterface.Models.Author>

@{
    ViewBag.Title = "Index";
}

<fieldset>
    <legend>Author List</legend>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        
            @foreach (var author in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(authorModel => author.first_name)</td>
                    <td>@Html.DisplayFor(authorModel => author.last_name)</td>
                </tr>
            }
        
    </table>

</fieldset>